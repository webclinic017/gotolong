			var id=$(this).attr('href');
			$('#lightbx-overlay').show();

			if($('body').height() > $(window).height()){
				$('#lightbx-overlay').css({'opacity':'0.5','height':$('body').height()});
			} else{
				$('#lightbx-overlay').css({'opacity':'0.5','height':$(window).height()});
			}

			$(id).show();
			$(id).center();
			return false;
			//$("#form-cont").show();
		});
	});

