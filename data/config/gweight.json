{
  "module": "gweight",
  "logging-level": "INFO",
  "debug-level": "1",
  "truncate-table": "Yes",
  "save-to-db": "Yes",
  "pretty-dump": "Yes",
  "input-files": [
    {
      "dir-cfg-key": "GLOBAL_DATA",
      "file": "gweight-data/gweight-data.csv"
    }
  ],
  "output-files": [
    {
      "dir-cfg-key": "GLOBAL_REPORTS",
      "file": "gweight-reports/gweight-reports.csv"
    }
  ]
}
